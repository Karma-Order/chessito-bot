{
    "env": {
        "node": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["airbnb-base", "prettier", "plugin:import/errors"],
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": ["import"],
    "rules": {
        "import/extensions": ["error", "ignorePackages"],
        "class-methods-use-this": 0
    }
}
